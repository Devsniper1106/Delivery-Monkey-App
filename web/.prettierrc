{
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "all",
  "printWidth": 100,
  "jsxBracketSameLine": true,
  "overrides": [
    {
      "files": "*.svelte",
      "options": {
        "parser": "svelte",
        "plugins": [
          "prettier-plugin-svelte"
        ]
      }
    }
  ],
  "pluginSearchDirs": [
    "."
  ]
}
